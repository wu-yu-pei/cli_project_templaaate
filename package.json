{
  "name": "@superyou/cli",
  "version": "1.0.0",
  "description": "一个cli 整合所有项目模版（vue、react、express、nest、koa等） 便于快速开发。",
  "main": "dist/index.js",
  "bin": {
    "initp": "dist/index.js"
  },
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin typescript",
    "dev": "rollup -c  rollup.config.ts --configPlugin typescript --watch"
